generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
}

model JobApplication {
  id            String   @id @default(cuid())
  company       String
  position      String
  positionType  String
  dateApplied   DateTime @default(now())
  link          String?
  location      String
  status        String   @default("Applied")
  createdAt     DateTime @default(now())
}
